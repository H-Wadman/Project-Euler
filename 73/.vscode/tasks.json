{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: clang++ build active file",
            "command": "/opt/homebrew/Cellar/llvm/17.0.4/bin/clang++",
            "args": [
                "-std=c++23",
                "-Wall",
                "-fcolor-diagnostics",
                "-fansi-escape-codes",
                "-fsanitize=address",
                "-I/opt/homebrew/Cellar/llvm/17.0.4/include/c++/v1/",
                "-L/opt/homebrew/Cellar/llvm/17.0.4/lib/c++",
                "-I/opt/homebrew/include/",
                "-L/opt/homebrew/lib/",
                "-L/usr/local/lib",
                "-lfilewrapper",
                "-g",
                "${fileDirname}/*.cpp",
                "-o",
                "${fileDirname}/out/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}
